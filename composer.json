{
    "name": "michielkempen/smoothy-package",
    "description": "A Laravel 5 package to kickstart your Smoothy application",
    "keywords": [
        "smoothy",
        "smoothy-package"
    ],
    "homepage": "https://github.com/michielkempen/smoothy-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Smoothy bvba",
            "email": "info@smoothy.nu",
            "homepage": "https://smoothy.nu"
        }
    ],
    "require": {
        "php": "^7.0",
        "cocur/slugify": "2.*",
        "devitek/yaml-translation": "3.*",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/http": "5.4.*",
        "illuminate/support": "5.4.*",
        "jakeasmith/http_build_url": "^1.0",
        "laravelcollective/html": "^5.2",
        "league/glide-laravel": "^1.0",
        "spatie/laravel-collection-macros": "^2.1",
        "spatie/laravel-cookie-consent": "^1.8",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "files": [
            "src/helpers/helpers.php"
        ],
        "psr-4": {
            "Smoothy\\": "src/smoothy"
        }
    },
    "config": {
        "sort-packages": true
    }
}
